var inc = 0.01;
var start = 0;
function setup() {
  createCanvas(200, 200);
  pixelDensity(1);
}

function draw() {
  var yoff = 0;
  loadPixels();
  for (var y = 0; y < width; y++) {
    var xoff = 0;
    for (var x = 0; x < height; x++) {
      var index = (x + y * width) * 4;
      var r = noise(xoff, yoff) * 255;
      xoff += inc;
      fill(r);
      rectx;
    }
    yoff += inc;
  }
  updatePixels();
}
