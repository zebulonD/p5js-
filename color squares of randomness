function setup() {
  createCanvas(400, 400);
}

function draw() {
  background(220);
  let c = color(random(0,255), random(0,255), random(0,255));
  x=0
  y=0
  s=50
  h=width/s
  l=height/s
  square(x,y,s)
  
  for(let i = 0;i<h;i++){
    for(let i = 0;i<l;i++){
      let c = color(random(0,255), random(0,255), random(0,255));
      fill(c);
      square(x,y,s)
      x+=s
    }
    y+=s
    x=0
  }
}
